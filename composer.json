{
	"name": "anod/gmail-imap",
	"description": "API to work with Gmail using IMAP built on top of Zend Imap Library",
	"keywords": ["gmail", "imap"],
	"homepage": "http://github.com/anod/gmail-imap-php",
	"authors": [
        {
            "name": "Alex Gavrishev",
            "email": "alex.gavrishev@gmail.com"
        }
     ],
	"license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mail": "2.10.*"
    },
	"require-dev": {
        "phpunit/phpunit": "5.7.27",
		"google/apiclient": "^2.0",
		"squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
	],  
	"autoload": {
        "psr-0": {"Anod": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    } 
}
